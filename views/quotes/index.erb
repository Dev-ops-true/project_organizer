
  <% @quotes.each do |item| %>
  <ul>
    <li><%= item[0] %></li>
    <li><%= item[1] %></li>
    <li><%= item[2] %></li>
    <li><%= item[3] %></li>
  </ul>
  <form action="/quotes/<%= item[0] %>" method="post">
    <input type='hidden' name='method' value='DELETE'/>
    <input type="submit" value="Delete" />
  </form>
  <% end %>
